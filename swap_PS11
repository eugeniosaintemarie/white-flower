# Define la ubicaci칩n donde se guardar치 el archivo
$logFolder = "C:\Logs"
if (!(Test-Path -Path $logFolder)) {
    New-Item -ItemType Directory -Path $logFolder
}

# Genera el nombre del archivo con fecha y hora
$timestamp = (Get-Date).ToString("yyyy-MM-dd_HH-mm-ss")
$logFile = "$logFolder\Log_$timestamp.txt"

# Escribe la hora en que termin칩 el script dentro del archivo
$endTime = Get-Date -Format "yyyy-MM-dd HH:mm:ss"
"El script finaliz칩 a las: $endTime" | Out-File -FilePath $logFile -Encoding UTF8
